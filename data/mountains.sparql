SELECT ?mountain ?mountainLabel ?height ?countryLabel ?visitors ?rockMaterialLabel
WHERE {
  ?mountain wdt:P31 wd:Q8502.  # Объект — гора
  ?mountain wdt:P2048 ?height.  # Высота
  ?mountain wdt:P17 ?country.  # Страна
  OPTIONAL { ?mountain wdt:P1174 ?visitors. }  # Число посетителей
  OPTIONAL { ?mountain wdt:P186 ?rockMaterial. }  # Материал скалы

  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],ru". }
}

